package com.dbms;

public class Project {
private int id;
private String name;
private int age;
private Double amount;
private String policy;
public Project() {
	super();
	this.id = id;
	this.name = name;
	this.age = age;
	this.amount = amount;
	this.policy = policy;
}

public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getAge() {
	return age;
}
public void setAge(int age) {
	this.age = age;
}
public int getAmount() {
	return amount;
}
public void setAmount(int amount) {
	this.amount = amount;
}
public String getPolicy() {
	return policy;
}
public void setPolicy(String policy) {
	this.policy = policy;
}
@Override
public String toString() {
	return "Project [id=" + id + ", name=" + name + ", age=" + age + ", amount=" + amount + ", policy=" + policy + "]";
}
}
